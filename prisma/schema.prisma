generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Wish {
    id       Int  @id @default(autoincrement())
    wishData Json   
}
model Image {
  id        Int     @id @default(autoincrement())
  url       String
  wishId    Int
  wish      Wish    @relation(fields: [wishId], references: [id])
  createdAt DateTime @default(now())
}